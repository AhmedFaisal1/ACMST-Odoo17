<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="acmst_admission_assets_frontend_js" model="ir.asset">
      <field name="name">ACMST Admission Frontend JS</field>
      <field name="bundle">web.assets_frontend</field>
      <field name="path">acmst_admission/static/src/js/phone.js</field>
      <field name="sequence" eval="100"/>
    </record>
    <record id="acmst_admission_assets_frontend_css" model="ir.asset">
      <field name="name">ACMST Admission RTL CSS</field>
      <field name="bundle">web.assets_frontend</field>
      <field name="path">acmst_admission/static/src/scss/rtl.css</field>
      <field name="sequence" eval="100"/>
    </record>

    <record id="acmst_admission_assets_report_css" model="ir.asset">
      <field name="name">ACMST Admission Report RTL CSS</field>
      <field name="bundle">web.report_assets_common</field>
      <field name="path">acmst_admission/static/src/scss/report_rtl.css</field>
      <field name="sequence" eval="100"/>
    </record>
  </data>
</odoo>
