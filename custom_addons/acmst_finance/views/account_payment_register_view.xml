<odoo>
    <record id="view_account_payment_register_form_inherit_acmst_bankak" model="ir.ui.view">
        <field name="name">account.payment.register.form.acmst.bankak</field>
        <field name="model">account.payment.register</field>
        <field name="inherit_id" ref="account.view_account_payment_register_form"/>
        <field name="arch" type="xml">
            <!-- place under Payment Date -->
            <xpath expr="//group/group/field[@name='payment_date']" position="after">
                <field name="bankak_pay_date"/>
                <field name="bankak_receipt" filename="bankak_receipt_filename" widget="image" class="oe_inline"/>
                <field name="bankak_receipt_filename" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>
