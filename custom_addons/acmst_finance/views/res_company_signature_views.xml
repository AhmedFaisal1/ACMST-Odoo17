<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_company_form_inherit_acmst_signatures" model="ir.ui.view">
    <field name="name">res.company.form.inherit.acmst.signatures</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <xpath expr="//group[@name='social_media']" position="after">
        <group string="ACMST Report Signatures">
          <field name="acmst_signature_invoice" widget="image" class="oe_avatar"/>
          <field name="acmst_signature_receipt" widget="image" class="oe_avatar"/>
          <field name="acmst_college_stamp" widget="image" class="oe_avatar"/>
        </group>
      </xpath>
    </field>
  </record>
</odoo>
