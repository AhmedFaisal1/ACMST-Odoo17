<odoo>
  <!-- Neutralize the old view: same xmlid, valid (but no-op) arch -->
  <record id="view_move_form_inherit_acmst_univid" model="ir.ui.view">
    <field name="name">account.move.form.inherit.acmst.univ_neutralized</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      <!-- no UI changes, just a benign attribute so the inherit is valid -->
      <xpath expr="//form" position="attributes">
        <attribute name="data-acmst">1</attribute>
      </xpath>
    </field>
  </record>
</odoo>
